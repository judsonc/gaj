<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.garcomajato" version="0.2.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Garçom a Jato</name>
    <description>
        Um projeto react com cordova.
    </description>
    <author email="judson.scosta@hotmail.com" href="http://judson.ejectufrn.com.br">
        Garçom a Jato Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="http://*/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~6.0.0" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-statusbar" spec="~2.2.0" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarBackgroundColor" value="#b71c1c" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <plugin name="cordova-plugin-headercolor" spec="~1.0" />
    <preference name="HeaderColor" value="#d50000" />
</widget>
